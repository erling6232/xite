#!/bin/sh

# $Id$

########## Save parameters ##########

if test -f "$paramOutFile"
then
	mv $paramOutFile $paramOutFile.old
fi

cat <<EOF > $paramOutFile
XITE_HOSTTYPE=$XITE_HOSTTYPE
XITE_HOME=$XITE_HOME
XITE_BIN_DEST=$XITE_BIN_DEST
XITE_LIB_DEST=$XITE_LIB_DEST
XITE_INC_DEST=$XITE_INC_DEST
XITE_MAN_DEST=$XITE_MAN_DEST
XITE_DOC_DEST=$XITE_DOC_DEST
X_INC_DIR=$X_INC_DIR
X_LIB_DIR=$X_LIB_DIR
TIFF_INC_DIR=$TIFF_INC_DIR
TIFF_LIB_DIR=$TIFF_LIB_DIR
PNM_INC_DIR=$PNM_INC_DIR
PNM_LIB_DIR=$PNM_LIB_DIR
MATLAB_LIB=$MATLAB_LIB
MATLAB_INC=$MATLAB_INC
MATLAB_MEX_DEST=$MATLAB_MEX_DEST
CC=$CC
EOF
